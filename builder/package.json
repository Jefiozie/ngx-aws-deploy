{
  "name": "@jefiozie/ngx-aws-deploy",
  "version": "2.0.1",
  "description": "Deploy rou Angular app to Amazon S3 directly from the Angular CLI",
  "main": "index.js",
  "builders": "./builders.json",
  "schematics": "./collection.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jefiozie/ngx-aws-deploy.git"
  },
  "scripts": {
    "build": "npm run copy && npm run build:schema && tsc",
    "build:schema": "json2ts ng-add/schema.json > ng-add/schema.d.ts && json2ts deploy/schema.json > deploy/schema.d.ts",
    "copy": "cp ../README.md ./ && cp ../LICENSE ./",
    "test": "jest",
    "release": "semantic-release --no-ci",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "angular",
    "schematics",
    "cli",
    "angular-cli",
    "amazon",
    "aws",
    "deploy",
    "ng deploy"
  ],
  "author": "Jeffrey Bosch",
  "license": "MIT",
  "devDependencies": {
    "@angular-devkit/architect": "0.1000.7",
    "@angular-devkit/schematics": "9.1.4",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.0.5",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/glob": "7.1.1",
    "@types/jest": "25.2.1",
    "@types/mime": "2.0.1",
    "@types/mime-types": "2.1.0",
    "@types/node": "13.13.4",
    "glob": "7.1.6",
    "jest": "25.5.3",
    "json-schema-to-typescript": "^9.1.0",
    "semantic-release": "17.0.7",
    "ts-jest": "25.4.0",
    "ts-node": "8.9.1",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@angular-devkit/architect": "^0.1000.7",
    "@angular-devkit/core": ">=8.0.0",
    "@angular-devkit/schematics": ">=8.0.0",
    "aws-sdk": "^2.599.0",
    "glob": "^7.1.6",
    "mime-types": "^2.1.26"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branches": [
      "master",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  }
}
