{
  "name": "@jefiozie/ngx-aws-deploy",
  "version": "1.0.4",
  "description": "Deploy rou Angular app to Amazon S3 directly from the Angular CLI",
  "main": "index.js",
  "builders": "./builders.json",
  "schematics": "./collection.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jefiozie/ngx-aws-deploy.git"
  },
  "scripts": {
    "build": "npm run copy && npm run build:schema && tsc",
    "build:schema": "json2ts ng-add/schema.json > ng-add/schema.d.ts && json2ts deploy/schema.json > deploy/schema.d.ts",
    "copy": "cp ../README.md ./ && cp ../LICENSE ./",
    "test:builder": "ts-node node_modules/jasmine/bin/jasmine deploy/index_spec.ts",
    "test:ngadd": "ts-node node_modules/jasmine/bin/jasmine ng-add/index_spec.ts",
    "test": "npm run test:builder && npm run test:ngadd"
  },
  "keywords": [
    "angular",
    "schematics",
    "cli",
    "angular-cli",
    "amazon",
    "aws",
    "deploy",
    "ng deploy"
  ],
  "author": "Jeffrey Bosch",
  "license": "MIT",
  "devDependencies": {
    "@angular-devkit/architect": "0.800.6",
    "@angular-devkit/schematics": "8.3.22",
    "@types/glob": "7.1.1",
    "@types/jasmine": "3.5.0",
    "@types/mime": "2.0.1",
    "@types/node": "11.15.4",
    "aws-sdk": "2.602.0",
    "glob": "7.1.6",
    "jasmine": "3.5.0",
    "jasmine-node": "3.0.0",
    "json-schema-to-typescript": "^8.0.1",
    "standard-version": "7.0.1",
    "ts-node": "8.6.2",
    "typescript": "3.7.4"
  },
  "dependencies": {
    "@angular-devkit/architect": "^0.800.0-beta.11",
    "@angular-devkit/core": ">=8.0.0",
    "@angular-devkit/schematics": ">=8.0.0",
    "aws-sdk": "^2.599.0",
    "glob": "^7.1.6"
  },
  "peerDependencies": {
    "@angular-devkit/architect": "^0.800.0-beta.11",
    "aws-sdk": "^2.599.0",
    "glob": "^7.1.6",
    "@angular-devkit/core": ">=8.0.0",
    "@angular-devkit/schematics": ">=8.0.0"
  }
}
