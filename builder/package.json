{
  "name": "@jefiozie/ngx-aws-deploy",
  "version": "1.1.0",
  "description": "Deploy rou Angular app to Amazon S3 directly from the Angular CLI",
  "main": "index.js",
  "builders": "./builders.json",
  "schematics": "./collection.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jefiozie/ngx-aws-deploy.git"
  },
  "scripts": {
    "build": "npm run copy && npm run build:schema && tsc",
    "build:schema": "json2ts ng-add/schema.json > ng-add/schema.d.ts && json2ts deploy/schema.json > deploy/schema.d.ts",
    "copy": "cp ../README.md ./ && cp ../LICENSE ./",
    "test": "jest",
    "release": "semantic-release --no-ci",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "angular",
    "schematics",
    "cli",
    "angular-cli",
    "amazon",
    "aws",
    "deploy",
    "ng deploy"
  ],
  "author": "Jeffrey Bosch",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "5.0.0",
    "@semantic-release/commit-analyzer": "8.0.0",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/npm": "7.0.0",
    "@semantic-release/release-notes-generator": "9.0.0",
    "semantic-release": "17.0.1",
    "@angular-devkit/architect": "0.803.23",
    "@angular-devkit/schematics": "8.3.23",
    "@types/glob": "7.1.1",
    "@types/jest": "24.9.1",
    "@types/mime": "2.0.1",
    "@types/node": "11.15.4",
    "glob": "7.1.6",
    "jest": "24.9.0",
    "json-schema-to-typescript": "8.0.1",
    "standard-version": "7.0.1",
    "ts-jest": "25.0.0",
    "ts-node": "8.6.2",
    "typescript": "3.7.4"
  },
  "dependencies": {
    "@angular-devkit/architect": "^0.803.0",
    "@angular-devkit/core": ">=8.0.0",
    "@angular-devkit/schematics": ">=8.0.0",
    "aws-sdk": "^2.599.0",
    "glob": "^7.1.6"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branch": "master"
  }
}
