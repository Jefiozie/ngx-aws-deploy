{
  "name": "ngx-aws-deploy",
  "description": "Deploy rou Angular app to Amazon S3 directly from the Angular CLI",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "8.3.4",
    "@commitlint/config-conventional": "8.3.4",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/commit-analyzer": "^7.0.0",
    "@semantic-release/git": "^8.0.0",
    "@semantic-release/npm": "^6.0.0",
    "@semantic-release/release-notes-generator": "^7.3.5",
    "husky": "4.0.9",
    "precise-commits": "1.0.2",
    "prettier": "1.19.1",
    "semantic-release": "^16.0.1"
  },
  "scripts": {
    "precise-commits": "precise-commits",
    "release": "semantic-release --no-ci"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-personal:Jefiozie/ngx-aws-deploy.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precise-commits",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "branch": "master"
  },
  "author": "Jeffrey Bosch <jefiozie.bosch@gmail.com>",
  "license": "MIT"
}
